{
  "name": "nestjs-azure-storage-blob",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/nestjs-azure-storage-blob/src",
  "projectType": "library",
  "release": {
    "version": {
      "generatorOptions": {
        "packageRoot": "dist\\{projectRoot}",
        "currentVersionResolver": "git-tag"
      }
    }
  },
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/nestjs-azure-storage-blob",
        "tsConfig": "libs/nestjs-azure-storage-blob/tsconfig.lib.json",
        "packageJson": "libs/nestjs-azure-storage-blob/package.json",
        "main": "libs/nestjs-azure-storage-blob/src/index.ts",
        "assets": ["libs/nestjs-azure-storage-blob/*.md"]
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist\\{projectRoot}"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/nestjs-azure-storage-blob/jest.config.ts"
      }
    },
    "example": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": true,
        "cwd": "libs/nestjs-azure-storage-blob/client",
        "commands": [{ "command": "next dev --port 3000", "color": "green" }]
      }
    }
  }
}
